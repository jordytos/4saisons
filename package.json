{
  "name": "4saisons",
  "version": "0.1.0",
  "author": "jordytos",
  "license": "MIT",
  "private": true,
  "repository": {
    "url": "https://github.com/jordytos/4saisons.git"
  },
  "source": "src/index.html",
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "format": "prettier --write {*,src/**/*}.{js,json,md,html}",
    "prepare": "husky install",
    "commit": "git-cz",
    "lint": "eslint src",
    "test": "jest --watch"
  },
  "dependencies": {
    "date-fns": "^2.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "babel-jest": "^27.5.1",
    "eslint": "^8.9.0",
    "eslint-config-react-app": "^7.0.0",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "parcel": "^2.3.1",
    "prettier": "^2.5.1"
  },
  "lint-staged": {
    "{*,src/**/*}.{js,json,md,html}": "prettier --write",
    "*.js": "eslint"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    }
  }
}
